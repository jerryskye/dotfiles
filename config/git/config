[commit]
	gpgsign = true
[tag]
	gpgsign = true
[rerere]
	enabled = true
[alias]
	s = status
	d = diff
	a = add
	br = branch
	co = checkout
	ci = commit
	l = log
	rb = rebase
	p = pull --no-rebase
[pull]
	rebase = true
[init]
	defaultBranch = main
[diff]
	colorMoved = default
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
